<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp">
        <Button
            android:id="@+id/btn_others_tan1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="탄생의 성소 1"/>
        <Button
            android:id="@+id/btn_others_tan2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="탄생의 성소 2"/>
        <Button
            android:id="@+id/btn_others_tan3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="탄생의 성소 3"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp">
        <Button
            android:id="@+id/btn_others_pa1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="파급의 성소 1"/>
        <Button
            android:id="@+id/btn_others_pa2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="파급의 성소 2"/>
        <Button
            android:id="@+id/btn_others_pa3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="파급의 성소 3"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp">
        <Button
            android:id="@+id/btn_others_so1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="소멸의 성소 1"/>
        <Button
            android:id="@+id/btn_others_so2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="소멸의 성소 2"/>
        <Button
            android:id="@+id/btn_others_so3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="소멸의 성소 3"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="110dp">
        <Button
            android:id="@+id/btn_others_tb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tb"/>
    </LinearLayout>
</LinearLayout>